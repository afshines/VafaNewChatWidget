{
  "name": "vafa-chat-widget",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vafa-chat-widget.umd.js",
  "module": "./dist/vafa-chat-widget.es.js",
  "exports": {
    ".": {
      "import": "./dist/vafa-chat-widget.es.js",
      "require": "./dist/vafa-chat-widget.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "LIBRARY_BUILD=true vite build",
    "build:app": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "socket.io-client": "^4.8.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "vite": "^6.3.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
